<?xml version="1.0"?>
<launch>
	<!-- get the arm parameters from the YAML files -->
	<rosparam command="load" file="$(find youbot_description)/controller/arm_joint_universal_control.yaml"/>
    <rosparam command="load" file="$(find raw_hardware_config)/$(env ROBOT)/config/arm.yaml"/>

	<!-- Start up ROS Console to see debugging values -->	
	<node pkg="rxtools" type="rxconsole" name="rxconsole"></node>

    <node pkg="hbrs_cartesian_trajectory_controller" type="cartesian_trajectory_controller" name="hbrs_cartesian_trajectory_controller" output="screen">
    </node>
</launch>
